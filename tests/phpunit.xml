<phpunit bootstrap="./Bootstrap.php" colors="true">
    <testsuite name="Value object test suite">
        <directory>.</directory>
    </testsuite>
    
    <groups>
        <exclude>
            <group>disable</group>
        </exclude>
    </groups>

    <filter>
        <whitelist>
            <directory suffix=".php">../library/</directory>
        </whitelist>
    </filter>
</phpunit>
